<%= simple_form_for(@contact) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :company_name, :url => autocomplete_company_name_contacts_path, :as => :autocomplete %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
